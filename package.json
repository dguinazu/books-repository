        "jquery": "^3.2",
        "jsdom": "^15.1.1",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.5",
        "mocha": "^6.2.0",
        "mochapack": "^1.1.4",
        "moment": "^2.29.4",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
